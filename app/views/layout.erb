    <script type="text/javascript">
    window.onload = function()
    {
      // fix for windows 8
      if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
        document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="pages/css/windows.chrome.fix.css" />'
      $.getJSON('/messages', function(response){
        $('#all_notifications').html('');
        if(response.length == 0){
          $('#all_notifications').html('<div align="center">No New Notification</div>');
        } else {  
          response.forEach(function(element, index, array){
            $('#all_notifications').append('<div class="notification-item  clearfix"><div class="heading"><a href="'+ element.link +'" class="text-complete pull-left"><i class="glyphicon glyphicon-comment"> </i> <span class="bold"> '+ element.title +' </span><span class="fs-12 m-l-10">'+ element.keyword +'</span></a><span class="pull-right time">'+ element.time +'</span></div></div>');
          });
        }
      })
    }
